package com.subarray;

import java.util.Arrays;

public class PrintAllPossibleSubSequence_Non_ContiguousForm {

	public static void main(String[] args) {

		char[] arr = { 'a', 'b', 'c', 'd' };
		int powerArray[] = new int[4];
		int e = 0;
		int on = 0, off = 0;
		while (e < arr.length) {
			int f = e;
			int power = 1;
			while (f != 0) {
				power = power * 2;
				--f;
			}
			powerArray[e] = power;
			on = on + power;
			e++;
		}
		System.out.println(Arrays.toString(powerArray));
		e = on;
		int arryTmp[] = new int[on];
		while (e > 0) {
			
		}

	}

}
