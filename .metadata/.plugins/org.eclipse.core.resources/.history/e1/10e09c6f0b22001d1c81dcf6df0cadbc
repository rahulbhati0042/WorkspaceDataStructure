package com.pigeonholesort;

public class PigeonHole {

	public static void main(String[] args) {
		int arr[] = { 9, 8, 7, 6, 5, 4, 3, 2, 1, 0 };
		int max =0,min=arr[0];
		int  e=0;
		while(e<arr.length){
			max  = Math.max(max, arr[e]);
			min = Math.min(min, arr[e]);
			e++;
		}
		System.out.println(max);
		System.out.println(min);
	}

}
