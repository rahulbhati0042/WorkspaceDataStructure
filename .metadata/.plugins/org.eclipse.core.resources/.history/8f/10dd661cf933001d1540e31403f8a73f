package com.subarray;

import java.util.Arrays;

public class PrintAllPossibleSubSequence_Non_ContiguousForm {

	public static void main(String[] args) {

		char[] arr = { 'a', 'b', 'c', 'd' };
		int tmpArr[] = new int[4];
		int e = 0;
		int power = 1;
		while (e < arr.length) {

			while (e != 0) {
				power = power * 2;
				--e;
			}
			tmpArr[e]=power;
		}
		System.out.println(Arrays.toString(tmpArr));

	}

}
