package com.arrayproblems;

public class MoveAllNegetiveAtEnd {

	public static void main(String[] args) {
		int arr[]= {-8, 9, 5, 10, 2, 6, -7, 7};
		
		int d =0;
		for(int i= arr.length-1;i>0;i++){
			if(arr[i]>0) d++;
			else{
				int k = i;
				for(int j=1;j<=d;j++){
					
					if(arr[k+j]>0){
						int tmp =arr[k];
						arr[k]=arr[k+j];
						arr[k+j]=tmp;
					}
					
				}
				
				
			}
			
			
			
			
		}

	}

}
