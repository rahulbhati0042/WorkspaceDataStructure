package com.singlylinkedlist;

public class ContainsLoop {
	static Node head;
	public static void main(String[] args) {
		head = new Node();
		head.data=1;
		createNodeList();
	}

	static void createNodeList() {
		Node n1 = new Node();
		Node n2 = new Node();
		Node n3 = new Node();
		Node n4 = new Node();
		n1.data = 10;
		n1.next = n2;
		n2.data = 20;
		n2.next = n3;
		n3.data = 30;
		n3.next = n4;
		n4.data = 40;
		n4.next = null;
		
		head.next = n1;
		Node j = head;
		while (j.next != null) {
			System.out.println(j.data);
			j = j.next;
		}
		System.out.println(j.data);
	}
	//int hasLoop()

}
