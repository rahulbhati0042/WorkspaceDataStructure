package com.arrayproblems;

import java.util.Arrays;

public class FindMissingAndRepeatingNumber_Method2_Sorting {

	public static void main(String[] args) {
		int arr[] = { 4, 3, 6, 2, 4, 1 };
		
		Arrays.sort(arr);
		boolean check = false;
		int count = 0;
		for (int i = 1; i <= arr.length; i++) {
			
			if(i != arr[i]){
				System.out.println("Missing :" + i);
			}
			
		}

	}

}
