package com.find.kth;

public class FindKthElement_MinHeap {
	static int arr[] = { 62, 5, 91, 54, 32, 79, 37, 63, 51, 49 };
	public static void main(String[] args) {
		
		getMeanHeap();
		System.out.println(arr);
	}
	private static void getMeanHeap() {
	
		int y = 1;
		int ci=0,ri=0;
		int tmp =0;
		while(y<arr.length){
			
			ci  = y;
			while(ci>0){
				ri  = (ci-1)/2;
				if(arr[ci]>arr[ri]){
					tmp = arr[ci];
					arr[ci] = arr[ri];
					arr[ri] = tmp;
				}
				ci  = ri;
			}
			y++;
		}
		
		
	}
	

}
