package com.stoogesort;

public class StoogeSort {
	static int arr[] = { 5, 6, 1 };

	public static void main(String[] args) {

		int lb = arr[0];
		int ub = arr[arr.length - 1];
		if (arr[lb] > arr[ub]) {
			int tmp = arr[lb];
			arr[lb] = arr[ub];
			arr[ub] = tmp;
		}
		int part = (arr.length / 3) + 1;
		if (part > 1) {
			int tmp[] = new int[part];
			int arr = left();

		}

	}

	static int[] left(int[] left) {
		return arr;

	}

	static int right(int right) {
		return right;

	}

}
