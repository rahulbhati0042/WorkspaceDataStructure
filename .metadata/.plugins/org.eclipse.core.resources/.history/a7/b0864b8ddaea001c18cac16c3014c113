package com.arrayproblems;

public class LeadersInArray {

	public static void main(String[] args) {
		//int a[] = {16,17,4,3,5,2} ;
		int a[] = {31,40,93,4, 98};
		boolean flag=true;
		for(int i =0;i<a.length;i++){
			for(int j=i+1;j<a.length;j++){
				if(a[i]>a[j]){
					flag=true;
					continue;
				}
				flag=false;
				break;
			}
			if(flag) System.out.println(a[i]);
		}
	}

}
