package com.arrayproblems;

public class SwapKthElements {

	public static void main(String[] args) {
		int arr[] = { 1, 2, 3, 4, 5, 6, 7, 8 };
		int n = arr.length;
		int k = 3;
		if (n > k) {
			int temp = arr[k - 1];
			arr[k - 1] = arr[n - k];
			arr[n - k] = temp;
		}
		System.out.println(arr);
	}

}
