package com.gcf;

public class EclideanAlgorithm_Recursion {
	public static void main(String[] args) {
		int numA = 18;
		int numB = 32;

		int e = 0;
		int f = 0;
		if (numA > numB) {
			e = numA;
			f = numB;
		} else {
			e = numB;
			f = numA;
		}
		recursion(e, f);
	}

	public static void recursion(int p, int q) {
		if (q == 0) {
			System.out.println(p);
			return;

		}
		recursion(q, p % q);
	}
}
