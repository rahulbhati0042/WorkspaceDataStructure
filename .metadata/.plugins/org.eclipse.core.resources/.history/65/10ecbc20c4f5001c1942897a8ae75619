package com.arrayproblems;

import java.util.Arrays;

public class FindMissingAndRepeatingNumber_Method2_Sorting {

	public static void main(String[] args) {
		int arr[] = { 4, 3, 6, 2, 1,1 };
		
		Arrays.sort(arr);
		boolean check = false;
		int count = 0;
		int duplicate=0;
		for (int i = 0; i < arr.length; i++) {
			
			if((i+1) != arr[i]){
				duplicate= arr[i];
				//break;
			}
			
		}
		System.out.println(duplicate);
		//int duplicate
		//int missing = 
	}

}
