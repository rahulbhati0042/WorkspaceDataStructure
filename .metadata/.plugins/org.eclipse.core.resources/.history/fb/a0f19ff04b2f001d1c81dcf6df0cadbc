package com.gcf;

public class EclideanAlgorithm {
	public static void main(String[] args) {
		int numA = 75;
		int numB = 50;

		int e = numB;
		int f = numA;
		if (numA > numB) {
			e = numA;
			f = numB;
		}

		while (f != 0 && (e / f) != 0) {
			int a = e / f;
			int b = e % f;
			e = f;
			f = b;
		}
		System.out.println(e);

	}
}
