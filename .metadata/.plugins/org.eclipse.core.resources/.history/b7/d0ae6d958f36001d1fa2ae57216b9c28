package com.subarray;

import java.util.Arrays;

public class TwoArrays_LCS_Length_Using_MatrixApproach {
	public static void main(String[] args) {
		int x[] = { 12, 3, 92, 12, 93, 78 };
		int y[] = { 54, 12, 93, 78, 93, 78 };

		int store[][] = new int[x.length + 1][y.length + 1];

		int maxSS = 0;

		for (int e = 1; e < x.length; e++) {

			for (int f = 1; f < y.length; f++) {

				if (x[e] == y[f]) {
					store[e][f] = store[e-1][f-1];
				} else {
					store[e][f] = x[e-1]>y[f-1]?store[e-1][f]:store[e][f-1];
					
				}

			}

		}
		for (int e = 0; e < store.length; e++)
			System.out.println(Arrays.toString(store[e]));
	}

}
