package com.recursion.problems;

public class LinearlyPrintNumbers {
	public static void main(String[] args) {
		LinearlyPrintNumbers a = new LinearlyPrintNumbers();
		a.linearlyPrint(1, 10);
	}

	public void linearlyPrint(int num, int n) {
		if (num > n) {
			return;
		}
		System.out.println(num);
		linearlyPrint(num++, n);
	}
}
