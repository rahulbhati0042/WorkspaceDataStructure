package com.gcf;

public class EclideanAlgorithm {
	public static void main(String[] args) {
		int numA = 20;
		int numB = 30;

		int e = 0;
		int f = 0;
		if (numA > numB) {
			e = numA;
			f = numB;
		} else {
			e = numB;
			f = numA;
		}

		while (e / f != 0) {
			int a = e /f;
			int b = e% f;
			 e = f;
			 f = b;
		}
		System.out.println(e);

	}
}
