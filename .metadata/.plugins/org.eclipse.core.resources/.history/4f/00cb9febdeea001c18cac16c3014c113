package com.arrayproblems;

public class LeadersInArray {

	public static void main(String[] args) {
		//int a[] = {16,17,4,3,5,2} ;
		int a[] = {31,40,93,4, 98};
		int i=0;
		for( i =0;i<a.length;i++){
			int j;
			for(j=i+1;j<a.length;j++){
				if(a[i]<=a[j]){
					break;
				}
			}
			 if (j == a.length) // the loop didn't break 
	                System.out.println(a[i]); 
		}
		
	}

}
