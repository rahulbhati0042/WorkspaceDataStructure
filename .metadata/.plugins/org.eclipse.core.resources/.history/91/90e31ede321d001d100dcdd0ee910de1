package com.shellsort;

import java.util.Arrays;

public class ShellSort_WhileLoop {

	public static void main(String[] args) {
		int a[] = { 44, 11, 99, 77, 33, 22, 55 };

		int y = 1;
		int z = 0;
		int num = 0;
		while (y <= a.length) {

			num = a[y];
			z = y - 1;
			while (z >= 0 && a[z] > num) {
				
				a[z+1]= a[z];
				z--;
				
			}
			a[z+1] = num;
			y++;

		}
		System.out.println(Arrays.toString(a));

	}

}
