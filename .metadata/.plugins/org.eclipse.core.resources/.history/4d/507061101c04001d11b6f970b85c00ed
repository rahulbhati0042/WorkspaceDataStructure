package com.recursion.problems;

public class FabonacciSeriesProblem {

	public static void main(String[] args) {
		int n = 2;
		int first = 0;
		int second = 1;
		for (int i = 0; i < n; i++) {

			if (n == 0 || n == 1) {
				System.out.println(n);
				return;
			} else {
				int temp = first;
				first = second;
				second = temp + first;
			}

		}
		System.out.println(second);
	}

}
