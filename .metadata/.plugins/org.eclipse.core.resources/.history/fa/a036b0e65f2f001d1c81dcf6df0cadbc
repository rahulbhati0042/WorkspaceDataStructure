package com.subarray;

public class MaxSumOfSubArray {

	public static void main(String[] args) {
		int arr[] = { 10, -2, 7, -6, -9, 3 };

		int maxSum = 0;
		int e = 0;
		int f = 0;
		while (e < arr.length) {

			f = 0;
			int sum = 0;
			while (f <= e) {
				sum = sum + arr[f];
				f++;
			}
			maxSum = Math.max(maxSum, sum);

			e++;
		}

	}

}
