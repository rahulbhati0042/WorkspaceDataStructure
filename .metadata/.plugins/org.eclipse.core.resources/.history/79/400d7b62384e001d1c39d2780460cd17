package com.bst;

import java.util.Stack;

public class BST_PreOrder {
	static Node root;
	static Stack<Node> stack = new Stack<>();

	public static void main(String[] args) {

		int arr[] = { 100, 50, 25, 20, 10, 35, 75, 70, 60, 90, 85, 95, 200,
				150, 175, 250, 300 };
		for (int i = 0; i < arr.length; i++)
			insertion(arr[i]);

		
			stack.add(root);
		
		while (!stack.isEmpty()) {
			Node n = stack.pop();
			System.out.println(n.data);
			if (n.right != null) {
				stack.add(n.right);
			}if (n.left != null) {
				stack.add(n.left);
			}
			
		}
	}

	public static void postOrder(Node t) {
		if (t == null)
			return;

		postOrder(t.left);
		postOrder(t.right);
		System.out.println(t.data);
	}

	public static void insertion(int data) {
		Node n = new Node();
		n.data = data;
		n.left = n.right = null;
		if (root == null) {
			root = n;
			return;
		}
		Node j = root;
		while (true) {
			if (j.data > n.data) {
				if (j.left == null) {
					j.left = n;
					break;
				} else
					j = j.left;

			} else {
				if (j.right == null) {
					j.right = n;
					break;
				} else
					j = j.right;
			}
		}

	}
}
